pkgname=calamity-calamares-conf
_destname="/etc"
pkgver=21.11
pkgrel=1
pkgdesc="calamares for calamity"
arch=('any')
url="https://github.com/vg08/${pkgname}"
license=('GPL3')
makedepends=('git')
depends=()
provides=("${pkgname}")
options=(!strip !emptydirs)
source=(${pkgname}::"git+${url}.git")
sha256sums=('SKIP')
package() {
	install -dm755 "${pkgdir}${_destname}"
	cp -r "${srcdir}/${pkgname}/${_destname}/"* "${pkgdir}${_destname}"
}

